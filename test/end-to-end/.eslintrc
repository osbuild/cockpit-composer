{
  "globals": {
    "document": true
  },
  "extends": [
    "airbnb",
    "plugin:jest/recommended"
  ],
  "env": {
    "jest": true
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "func-names": ["error", "never"],
    "import/no-unresolved": ["error", { "ignore": ["nightmare'"] }],
    "max-len": ["error", 130]
  }
}
