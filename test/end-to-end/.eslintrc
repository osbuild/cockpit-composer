{
  "globals": {
    "document": true,
    "browser": true,
    "$": true,
    "$$": true
  },
  "extends": [
    "airbnb"
  ],
  "env": {
    "mocha": true
  },
  "plugins": [
    "mocha"
  ],
  "rules": {
    "mocha/no-exclusive-tests": "error",
    "func-names": ["error", "never"],
    "no-eval": "off",
    "no-unused-vars": "off",
    "import/no-unresolved": ["error", { "ignore": ["copy-paste'"] }],
    "max-len": ["error", 130],
    "no-console": ["error", { "allow": ["error"] }]
  }
}
