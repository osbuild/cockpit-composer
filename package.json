    "postcss-custom-properties": "^5.0.1",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-flexbugs-fixes": "^2.0.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-media-minmax": "^2.1.2",
    "postcss-nesting": "^2.3.1",
    "postcss-selector-matches": "^2.0.1",
    "postcss-selector-not": "^2.0.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-test-renderer": "^15.6.1",
    "s3": "^4.4.0",
    "style-loader": "^0.13.1",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "jest": {
    "testRegex": "(/test/unit/.*\\.spec.js)$",
    "setupFiles": [
      "<rootDir>/test/unit/env.js"
    ],
    "collectCoverageFrom": [
      "**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/",
      "<rootDir>/public/",
      "<rootDir>/webpack.config.js"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "eslint:recommended"
    ],
    "rules": {
      "max-len": [
        "error",
        130
      ],
      "no-console": 0
    },
    "env": {
      "jest": true
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single"
    }
  },
  "scripts": {
    "eslint": "eslint components core data pages test utils main.js run.js webpack.config.js",
    "stylelint": "stylelint \"components/**/*.css\" \"pages/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "clean": "node run clean",
    "build": "node run build",
    "build:debug": "node run build --debug",
    "publish": "node run publish",
    "publish:debug": "node run publish --debug",
    "start": "node run",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage"
  }
}
